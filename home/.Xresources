urxvt.geometry: 96x24
!urxvt.transparent: true
!urxvt.shading: 20
urxvt.scrollBar: false
urxvt.cursorBlink: true
urxvt.cursorUnderline: true
urxvt.jumpScroll: false
urxvt.scrollWithBuffers: true

! clickable links
urxvt.perl-ext-common: default,matcher
urxvt.url-launcher: /usr/bin/google-chrome
urxvt.matcher.button: 1

! font
urxvt.font: xft:monospace:size=10
urxvt.imLocale: en_US.UTF-8

!dpi
!Xft.dpi: 176
!Xft.autohint: 0
!Xft.lcdfilter:  lcddefault
!Xft.hintstyle:  hintfull
!Xft.hinting: 1
!Xft.antialias: 1
!Xft.rgba: rgb

! special
*.foreground:   #f8f9fa
*.background:   #212529
*.cursorColor:  #f8f9fa

! black
*.color0:       #212529
*.color8:       #343a40

! red
*.color1:       #c92a2a
*.color9:       #ff6b6b

! green
*.color2:       #2b8a3e
*.color10:      #51cf66

! yellow
*.color3:       #fcc419
*.color11:      #ffec99

! blue
*.color4:       #1864ab
*.color12:      #339af0

! magenta
*.color5:       #087f5b
*.color13:      #20c997

! cyan
*.color6:       #0b7285
*.color14:      #22b8cf

! white
*.color7:       #ced4da
*.color15:      #f8f9fa
